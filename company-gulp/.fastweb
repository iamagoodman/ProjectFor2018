{
  "hash":false,
  "mini":false,
  "del":"dist",
  "style":{
    "type":"less",
    "src":"src/**/*.less",
    "dest":"dist"
  },
  "js":{
    "src":"src/js/**/*",
    "dest":"dist/js"
  },
  "img":{
    "src":"src/img/*",
    "dest":"dist/img"
  },
  "html":{
    "src":"src/pages/*.html",
    "dest":"dist"
  },
  "server":{
    "baseDir":"./dist",
    "directory":true,
    "files":"./dist/**/*",
    "port":8088
  }
}
